# Modified by H.Kakugawa to use libtool

LIBTOOL   = ../libtool
SHELL     = /bin/sh
CC        = @CC@
CPP       = @CPP@
CFLAGS    = @CFLAGS@
OPTIONS   = @OPTIONS@
X_LIBS    = @X_LIBS@
TOPSRC    = @top_srcdir@
XPM_LIB   = -lXpm
XLIB      = @X_PRE_LIBS@ -lXext -lX11 @X_EXTRA_LIBS@
LDFLAGS   = @LDFLAGS@
LDLIBS    = @LDLIBS@ 
AR        = ar rc
RANLIB    = @RANLIB@
RM        = rm -f
X_CFLAGS  = @X_CFLAGS@
YACC      = @YACC@
LEX       = @LEX@
LEXLIB    = @LEXLIB@
ALLCFLAGS = $(CFLAGS)
SUBMAKE   = $(MAKE) 'CC=$(CC)' 'CFLAGS=$(ALLCFLAGS)' 'OPTIONS=$(OPTIONS)'
@SET_MAKE@

INSTALL         = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA    = @INSTALL_DATA@
MKINSTALLDIRS   = @top_srcdir@/ac-tools/mkinstalldirs
prefix          = @prefix@
exec_prefix     = @exec_prefix@
includedir      = @includedir@
bindir          = @bindir@
datadir         = @datadir@
libdir          = @libdir@
mandir          = @mandir@/man1
manext          = .1
no_x            = @no_x@


# targets to build 

MAIN_TARGET = @MAIN_TARGET@

OBJS = \
	type1afm.lo 

SRCS = \
	type1afm.c 

T1LIB =  ../lib/libt1.la 


all: type1afm

type1afm: $(OBJS) ../lib/t1lib.h
	$(LIBTOOL) --mode=link \
		$(CC) -o type1afm $(LDFLAGS) $(OBJS) $(T1LIB) $(LDLIBS)

.SUFFIXES: .lo
.c.lo:
	$(LIBTOOL) --mode=compile \
		$(CC) -c $(CPPFLAGS) $(CFLAGS) $<

.PHONY: clean

dependencies: dummy 
	gcc -MM *.c | sed 's/\.o:/.lo:/g' > .dependencies 

clean: dummy
	$(RM) type1afm *.o *.lo .libs/* *~ \#*\# *.log *.bak
	-rmdir .libs

install: dummy
	$(MKINSTALLDIRS) $(DESTDIR)$(bindir)
	$(LIBTOOL) --mode=install \
		$(INSTALL_PROGRAM) type1afm $(DESTDIR)$(bindir)/type1afm


uninstall: dummy
	$(LIBTOOL) --mode=uninstall \
		$(RM) $(DESTDIR)$(bindir)/type1afm	


dummy:

# Dependencies of object files 
include .dependencies

