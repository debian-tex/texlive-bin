# deactivate the xdvi.bin alternative
case "$1" in 
  upgrade)
    ;;
  *)
    update-alternatives --remove xdvi.bin /usr/bin/xdvi-xaw
    update-alternatives --remove bibtex /usr/bin/bibtex.original
    ;;
esac

